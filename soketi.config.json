{
  "debug": true,
  "port": 6001,
  "appManager": {
    "driver": "array",
    "options": {
      "apps": [
        {
          "id": "shopping-agent",
          "key": "shopping-agent-key",
          "secret": "shopping-agent-secret",
          "webhooks": [],
          "maxConnections": -1,
          "enableClientMessages": true,
          "enabled": true,
          "maxBackendEventsPerSecond": -1,
          "maxClientEventsPerSecond": -1,
          "maxReadRequestsPerSecond": -1,
          "enableUserAuthentication": true
        }
      ]
    }
  },
  "cors": {
    "credentials": true,
    "origin": ["*"],
    "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "allowedHeaders": [
      "Origin",
      "Content-Type", 
      "X-Auth-Token",
      "X-Requested-With",
      "Accept", 
      "Authorization",
      "X-CSRF-TOKEN",
      "XSRF-TOKEN",
      "X-Socket-Id"
    ]
  }
}